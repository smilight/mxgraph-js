{
  "name": "mxgraph-js",
  "version": "4.2.3",
  "description": "mxgraph-js model",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./init": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js",
      "default": "./dist/init.js"
    },
    "./resources": {
      "types": "./dist/resources/index.d.ts",
      "import": "./dist/resources/index.js",
      "default": "./dist/resources/index.js"
    },
    "./resources/*": "./dist/resources/*",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "sideEffects": [
    "./dist/init.*",
    "./dist/resources/**"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "node ./scripts/clean.mjs",
    "generate": "node ./scripts/generate-entrypoints.mjs",
    "generate:dts": "node ./scripts/generate-dts-from-jsdoc.mjs",
    "build": "npm run clean && npm run generate && npm run generate:dts && npm run compile && node ./scripts/post-compile.mjs && node ./scripts/build-legacy.mjs",
    "compile": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "dist": "npm run build",
    "deploy": "gulp --gulpfile gulpfile.cjs deploy",
    "release": "npm run build && npm version patch -m \"chore: release %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smilight/mxgraph-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smilight/mxgraph-js/issues"
  },
  "homepage": "https://github.com/smilight/mxgraph-js#readme",
  "devDependencies": {
    "del": "^2.2.2",
    "esbuild": "^0.27.2",
    "gh-pages": "^0.12.0",
    "gulp": "^4.0.2",
    "gulp-usemin": "^0.3.26",
    "typescript": "^5.4.0",
    "webpack": "^1.14.0"
  }
}
